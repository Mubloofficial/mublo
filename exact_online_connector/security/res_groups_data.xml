<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="module_exact_online_connector" model="ir.module.category">
        <field name="name">Exact Online Connector</field>
    </record>

    <record id="group_exact_online_connector_user" model="res.groups">
        <field name="name">User</field>
        <field name="category_id" ref="module_exact_online_connector"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>
    <record id="group_exact_online_connector_manager" model="res.groups">
        <field name="name">Manager</field>
        <field name="implied_ids" eval="[(4, ref('group_exact_online_connector_user'))]"/>
        <field name="category_id" ref="module_exact_online_connector"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>

</odoo>
